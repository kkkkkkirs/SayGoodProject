<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.saygood.soft.model">
    <class name="ScanViewModel" table="scan_view">
        <id name="id" column="id" type="java.lang.Integer">
            <generator class="increment"/>
        </id>
        <property name="title" column="title" type="java.lang.String" />
        <property name="content" column="content" type="java.lang.String" />
        <property name="publisher" column="publisher" type="java.lang.String" />
        <property name="scanUrl" column="scanUrl" type="java.lang.String" />
    </class>
</hibernate-mapping>